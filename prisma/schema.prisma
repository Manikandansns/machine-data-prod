generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model MachineData {
  id              String   @id @default(uuid())
  temp    Float?
  humidity        Float?
  volume    Float?
  percentage     Float?
  voltage         Float?
  current         Float?
  power           Float?
  energy          Float?
  frequency       Float?
  pf     Float?
  plc_error_id   Int?
  createdAt       DateTime @default(now())
}
